/* 
Phrase for expressing a date in the relative past. E.g. "12 minutes ago"
<relative time difference> <date component name> ago
*/
"formatTemplateForRelativePastDatePhrase" = "%d%@";

/* 
Phrase for expressing a date in the relative future. E.g. "in 12 minutes"
in <relative time difference> <date component name>
*/
"formatTemplateForRelativeFutureDatePhrase" = "in %d %@";

/* localized date component names and plural forms */
"now" = "now";
"year" = "y";
"years" = "y";
"month" = "M";
"months" = "M";
"week" = "w";
"weeks" = "w";
"day" = "d";
"days" = "d";
"hour" = "h";
"hours" = "h";
"minute" = "m";
"minutes" = "m";
"second" = "s";
"seconds" = "s";
